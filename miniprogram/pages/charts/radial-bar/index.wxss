/* pages/chart/line/index.wxss */
.container {
    margin: 0 auto;
    background-color: #151515;
    border-radius: 5px;
    box-shadow: 0px 0px 1px 0px #06060d;
    padding: 50px;
}

ff-canvas {
  width: 100%;
  height: 100%;
}

.container {
  position: absolute;
  top: 0;
  bottom: 0;
  left: 0;
  right: 0;

  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  box-sizing: border-box;
}